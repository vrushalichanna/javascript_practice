function isPowerOTwoBitwise(n){
if(n<1) {
	return false
} 

return (n & (n-1)) === 0;
}

console.log(isPowerOTwoBitwise(1))
console.log(isPowerOTwoBitwise(2))
console.log(isPowerOTwoBitwise(3))
console.log(isPowerOTwoBitwise(4))
console.log(isPowerOTwoBitwise(5))
console.log(isPowerOTwoBitwise(6))
console.log(isPowerOTwoBitwise(8))

//Big O = O[1]
